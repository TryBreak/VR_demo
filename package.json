{
  "name": "one-react-app",
  "version": "0.2.0",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 3 versions"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/mnt/c/Mark/MyProject/Public_React_WebApp/node_modules/jest-watch-typeahead/filename.js",
      "/mnt/c/Mark/MyProject/Public_React_WebApp/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "dependencies": {
    "@babel/core": "7.4.3",
    "@svgr/webpack": "4.2.0",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "dotenv": "7.0.0",
    "dotenv-expand": "5.1.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "^24.7.1",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "^0.3.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.6.0",
    "mobx": "^4.9.4",
    "mobx-react": "^5.4.3",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.4.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "store": "^2.0.12",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-dev-server": "3.3.1",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.2.0"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.6.1",
    "eslint-plugin-import": "2.17.1",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "koa": "^2.7.0",
    "koa-static": "^5.0.0",
    "prettier": "1.17.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "npm run remove:build & node scripts/build.js",
    "test": "node scripts/test.js",
    "local-serve": "node scripts/server.js",
    "remove:build": "rm -rf ./build"
  },
  "proxy": "http://192.168.2.29:50051/",
  "homepage": ""
}
